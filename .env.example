# Example environment file (do NOT commit secrets)
# Copy this to `.env.dev` for development, and to `.env.prod` for production.

# Database
DB_HOST=postgres
DB_PORT=5432
DB_NAME=cyberquiz
DB_USER=cyberquiz
DB_PASSWORD=your-dev-db-password

# JWT
JWT_SECRET=your-very-long-jwt-secret-with-minimum-32-chars

# Admin user
ADMIN_EMAIL=admin@your-domain.example
ADMIN_PASSWORD=choose-a-strong-password

# Services
OLLAMA_BASE_URL=http://ollama:11434
QDRANT_URL=http://qdrant:6333
ALLOW_EXTERNAL_AI=false
